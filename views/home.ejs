<%- include("partials/header") -%>

<form action="/" method="post">
  <div class="jumbotronHome jumbotron-fluid p-5 mb-4 bg-light rounded-3 center">
    <div class="container-fluid py-5 row">
      <div class="column">
        <img class="logo_home" src="/images/vt_logo.png" alt="Logo image of VolunTier">
        <h1 class="display-5 fw-bold">What's your volunteer?</h1>
        <p class="col-sm-16 fs-4">VolunTier allows easier access for kids through young adults to quickly
          find volunteer opportunities in nearby areas or even remotely!</p>
        <button type="button" class="btn btn-success" type="submit">Vounteer</button>
      </div>
      <div class="column">

        <div class="carousel slide carousel-fade" data-bs-ride="carousel">
          <div class="carousel-inner">
            <div class="carousel-item active">
              <img src="/images/hands.jfif" alt="hand picture">
            </div>
            <div class="carousel-item">
              <img src="/images/hwijin.jpg" alt="...">
            </div>
            <div class="carousel-item">
              <img src="/images/matt.jpg" alt="...">
            </div>
          </div>
          <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
          </button>
          <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleFade" data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
          </button>
        </div>

<!-- End Carousel -->
      </div>
    </div>
  </div>
</form>







<%- include("partials/footer") -%>
